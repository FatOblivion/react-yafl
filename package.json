{
  "name": "kwik-form",
  "version": "1.0.0",
  "description": "A library for creating forms in react using react's new context api",
  "main": "index.js",
  "scripts": {
    "build": "rm ./lib/* && tsc",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib",
    "validate"
  ],
  "keywords": [
    "react",
    "form",
    "context"
  ],
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
