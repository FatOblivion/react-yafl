{
  "name": "kwik-form",
  "version": "1.0.0",
  "description": "A library for creating forms in react using react's new context api",
  "main": "dist/react-form-provider.js",
  "module": "dist/react-form-provider.es6.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
		"prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.base.json && rollup -c && rimraf compiled",
    "watch": "tsc --watch",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "git add"
    ]
  },
  "author": "Stuart Bourhill",
  "license": "MIT",
  "dependencies": {},
  "keywords": [
    "react",
    "form",
    "context"
  ],
  "peerDependencies": {
    "react": ">=16.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "8.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
